{
    "name": "codebykyle/laravel-rabbitmq-model-publisher",
    "description": "Publish model events to RabbitMQ",
    "keywords": [
        "laravel",
        "rabbitmq",
        "amqp",
        "microservice",
        "crud"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "bschmitt/laravel-amqp": "^2.0",
        "laravel/framework": "~5.8|^6.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "CodeByKyle\\RabbitMqModelPublisher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeByKyle\\RabbitMqModelPublisher\\RabbitMqModelPublisherProvider"
            ],
            "aliases": {
                "RabbitMqModelPublisher": "CodeByKyle\\RabbitMqModelPublisher\\Facades\\RabbitMqModelPublisher"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
